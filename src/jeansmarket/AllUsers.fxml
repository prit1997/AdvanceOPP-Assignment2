<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="478.0" prefWidth="707.0" stylesheets="@../css/file.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="jeansmarket.AllUsersController">
   <children>
      <ImageView fitHeight="95.0" fitWidth="200.0" layoutX="7.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/Arvind.png" />
         </image>
      </ImageView>
      <Label layoutX="413.0" layoutY="21.0" text="All Users Data">
         <font>
            <Font name="Arial Black" size="36.0" />
         </font>
      </Label>
      <TableView fx:id="userTable" layoutX="54.0" layoutY="89.0" prefHeight="200.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="userIDColumn" prefWidth="88.0" text="User ID " />
          <TableColumn fx:id="firstNameColumn" prefWidth="119.0" text="First Name" />
            <TableColumn fx:id="lastNameColumn" prefWidth="116.0" text="Last Name" />
            <TableColumn fx:id="phoneColumn" minWidth="0.0" prefWidth="135.0" text="Phone Number" />
            <TableColumn fx:id="birthdayColumn" minWidth="0.0" prefWidth="140.0" text="Birth Date" />
        </columns>
      </TableView>
      <Button fx:id="createUserButton" layoutX="132.0" layoutY="319.0" mnemonicParsing="false" onAction="#newUserButtonPushed" text="Create New User">
         <font>
            <Font name="AR CENA" size="18.0" />
         </font>
      </Button>
      <Button fx:id="editUserButton" layoutX="462.0" layoutY="319.0" mnemonicParsing="false" onAction="#editButtonPushed" text="Edit New User">
         <font>
            <Font name="AR CENA" size="18.0" />
         </font>
      </Button>
      <Button layoutX="613.0" layoutY="428.0" mnemonicParsing="false" onAction="#logoutButtonPushed" text="Logout">
         <font>
            <Font name="AR CENA" size="18.0" />
         </font>
      </Button>
      <Button layoutX="29.0" layoutY="428.0" mnemonicParsing="false" onAction="#storeButtonPushed" text="Store">
         <font>
            <Font name="AR CENA" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
